{
    "name": "reactflow-sidebar-template",
    "version": "1.0.0",
    "scripts": {
      "dev": "concurrently \"yarn run dev:frontend\" \"yarn run dev:backend\"",
      "dev:frontend": "cd frontend && yarn dev",
      "dev:backend": "cd backend && uvicorn app.main:app --reload",
      "install:all": "yarn install && cd frontend && yarn install && cd ../backend && pip install -r requirements.txt"
    },
    "devDependencies": {
      "concurrently": "^6.2.0"
    }
  }